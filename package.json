{
  "name": "livraria-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "build:ios": "cd ios && xcodebuild -workspace LivrariaApp.xcworkspace -scheme LivrariaApp -configuration Release -sdk iphoneos",
    "build:ios:simulator": "cd ios && xcodebuild -workspace LivrariaApp.xcworkspace -scheme LivrariaApp -configuration Release -sdk iphonesimulator",
    "build:ios:device": "cd ios && xcodebuild -workspace LivrariaApp.xcworkspace -scheme LivrariaApp -configuration Release -sdk iphoneos",
    "build:ios:device": "cd ios && xcodebuild -workspace LivrariaApp.xcworkspace -scheme LivrariaApp -configuration Release -sdk iphonesimulator",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:android:device": "cd android && ./gradlew assembleDebug",
    "build:android:simulator": "cd android && ./gradlew assembleDebug",
    "lint": "eslint .",
    "test": "jest"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "^0.72.0",
    "@expo/webpack-config": "^18.0.1",
    "expo": "^48.0.0",
    "expo-status-bar": "~1.4.4",
    "async": "^3.2.4",
    "axios": "^1.4.0",
    "react-native-web": "~0.19.6"

  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@babel/runtime": "^6.22.0",
    "@react-native/babel-preset": "^0.72.0",
    "babel-jest": "^29.7.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "metro-react-native-babel-preset": "^0.72.0",
    "@react-native-community/cli": "latest",
    "@react-native-community/cli-platform-ios": "latest",
    "@react-native-community/cli-platform-android": "latest"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "last 1 edge's version",
      "last 1 ie's version",
      "last 1 ios's version",
      "last 1 android's version",
      "last 1 opera's version"
    ]
  },
  "jest": {
    "preset": "react-native"
  }
}